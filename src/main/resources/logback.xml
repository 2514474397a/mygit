<?xml version="1.0" encoding="UTF-8"?>
<configuration>

       <logger name="com.dcits.example.demo.Controller" level="info"/>
      <logger name="com.dcits.example.demo.Controller.testController" level="debug"/>

<appender name="std" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
        <pattern>
            %d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n
        </pattern>
    </encoder>
</appender>


<root level="debug"/>


</configuration>

